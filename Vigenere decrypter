alphabet = "ABCDEFGHIJKLMNOPQRSTUVWXYZ"

Y1enc = "UBNUCAUJCNMCHLMWQBJFCHWHNBYBBCZMYLBYIJICNINJYNQYSCBJIIAIYMYMMSYNGOYUULNCAMNBLIYYBNUSYNHLYLYMYQVJAQBSMYZLXQNMANLHHHCNCYUXHMWYYIBFUZNCUCQCILGZICCCCOVYUXQJLGYNUNCHLICNPCMXOJCFNECLUIYJYBIUFCNWYMYYI"
Y2enc = "ZRVABVAUPNVANRVNNVRRANBTBBBRRQGGGFRRCYVAUHVNFRUGAPERAZBSURCVGFVVREAFPNUBVNUNRQQZRVAFKYRFAFURAVRGNURXVEGINRBUGBIGVNAEBJFORABNFHRGYVVAQNNAARBBYPATGEFBFORVFCQUPNAQGYPHRGJBCYTVUVAVVUEBQVJILPFNGGFFJ"
Y3enc = "IUKITALGQZBOAMLNBVWOOVUWNCVWZMPZNBGBMMVBMAUAIZQPWMMZAQCBWBPKABVIIMBVKBMTAGMDXCBIUKAQIGXWBBWBQTMGQMVQVTPIVVEQWAMPFLQIVISGPMTOIBXQQOVIMQAOENCZQIBCARWVSCZSKIBMSJOLGQAVZQPGMMPSIAOALIMVQAOMDMJCPIIW"
Y4enc = "MTBUHMTLLXMBMMXXVZIHBWBNTLMMLHXXBMLPHZZHAXXLYTEXMMXLVGMANARBTABELFPMNXUHMLRXKVAMTBTYVHXGXANAMEFTGGKLZXTKWMTGGXBXHFLMALXHBPENUMHVGAZVFGAMXTLIMEKXHHFXXMXXHKHULUTBIMTBLXRHHYMXMLXLMOLWGEKEHBXLXDKE"

Y1 = list(Y1enc)
Y2 = list(Y2enc)
Y3 = list(Y3enc)
Y4 = list(Y4enc)

Y1dec = ''.join([alphabet[(((alphabet.find(x))+6)%26)]  for x in Y1])
Y2dec = ''.join([alphabet[(((alphabet.find(x))+13)%26)] for x in Y2])
Y3dec = ''.join([alphabet[(((alphabet.find(x))+18)%26)] for x in Y3])
Y4dec = ''.join([alphabet[(((alphabet.find(x))+7)%26)]  for x in Y4])

x=0

while True:
  if x >= len(Y1enc):
    break
  else:
    print(Y1dec[x], end ="")
    
  if x >= len(Y2enc):
    break
  else:
    print(Y2dec[x], end ="")
    
  if x >= len(Y3enc):
    break
  else:
    print(Y3dec[x], end ="")  
    
  if x >= len(Y4enc):
    break
  else:
    print(Y4dec[x], end ="")
    
  x += 1
